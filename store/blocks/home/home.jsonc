      //"maxHeight": "280px",
      "blockClass": "lastColImage"
    }
  },
  // SHELF 1
  "shelf#home1": {
    "blocks": [
      "product-summary.shelf"
    ],
    "props": {
      "orderBy": "OrderByTopSaleDESC",
      "paginationDotsVisibility": "desktopOnly",
      "productList": {
        "maxItems": 20,
        "itemsPerPage": 4,
        "minItemsPerPage": 1,
        "scroll": "BY_PAGE",
        "arrows": true,
        "titleText": "OS QUERIDINHOS "
      }
    }
  },
