    ],
    "props": {
      "blockClass": "rowDeals"
    }
  },
  "flex-layout.col#dealsCol3": {
    "children": [
      "image#Infantil"
    ],
    "props": {
      "width": "25%"
    }
  },
  "flex-layout.col#dealsCol4": {
    "children": [
      "image#Belga"
    ],
    "props": {
      "width": "55%"
    }
  },
  "image#Infantil": {
    "props": {
      "src": "https://niazi.vteximg.com.br/arquivos/3Linhas.jpg"
